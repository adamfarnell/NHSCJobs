<div class = "container-fluid">
    <div class = "row panel">
        <% @priorities.each do |t| %>
        <div class ="col-xs-2 text-center panel">
            <p><%= t.start_time %></p>
            <p><%= t.pvalue %></p>
        </div>
        <% end %>
    </div>
    <div class = "row panel">
        <%= month_calendar events: @priorities do |date, priorities| %>
            <%= date %>
            <% priorities.each do |priority| %>
                <div>
                    <%= priority.pvalue %>
                </div>
            <% end %>
        <% end %>

        <%= week_calendar do |date| %>
            <%= date %>
        <% end %>
        <%= link_to "Edit", edit_user_path(@user) %>
    </div>
</div>